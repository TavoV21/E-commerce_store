pipeline{
    agent any
     environment {
        EMAIL='gusvillada21@gmail.com'
        NICKNAME='MAJIN'
        DEV_ENVIRONMENT= "${env.NODE_ENV}"
    }
    stages{
        stage('primer pipeline'){
            steps{
                echo "ejecutando mi primer pipeline"
            }
        }
        stage('imprima esta informacion'){
            steps{
                echo "su correo es $EMAIL"
                echo "su apodo es $NICKNAME"
                echo "su entorno de desarrollo $DEV_ENVIRONMENT"
            }
        }
    }
}